html,
body
  font-family $os
  color black
body
  overflow-x hidden

.content
  padding 160px 65px 0
  @media $minlDesktopMax
    padding 160px 160px 0
  @media $desktopMin
    padding 160px 35px 0
  @media $LandscapeMax
    padding 130px 35px 0
  
.content,
.text
  font-family $os
  h1:not([class])
    font-family $officina
    fontify 72px 400 76px
    margin 63px 0 35px
  h2:not([class]),
  h3:not([class]),
  h4:not([class])
    font-family $officina
    fontify 30px 400 36px
    margin 75px 0 23px
  img:not([class])
    margin 5px 0
    max-width 100%
  ul:not([class])
    list-style  none
    margin  23px 0
  ul:not([class]) li
    pl 40px
    pos relative
    fontify 15px 400 21px
    @media $LandscapeMax
      fontify 14px 400 21px
  ul:not([class]) li + li
    mt 18px
  ul:not([class]) li:before
    content ""
    pos absolute
    l 1px
    t 11px
    size 20px 1px
    bgc $dark
  ol:not([class])
    list-style none
    margin 23px 0
    counter-reset myCounter
  ol:not([class]) li
    pl 30px
    pos relative
    fontify 15px 400 21px
    @media $LandscapeMax
      fontify 14px 400 21px
  ol:not([class]) li + li
    mt 24px
  ol:not([class]) li:before
    counter-increment myCounter
    content counter(myCounter)"."	
    pos absolute
    l 0
    t 0
    w 30px
    pr 9px
    color $orange
    fontify 15px 400 21px
    @media $LandscapeMax
      fontify 14px 400 21px
  p:not([class])
    fontify 15px 400 21px
    margin 18px 0
    @media $LandscapeMax
      fontify 14px 400 21px
  a:not([class])
    color black
    border-bottom 2px solid $orange
    transition .7s border
    &:hover
      border-color transparent
 
  *:not([class]):not(span):first-child
    mt 0 !important
  *:not([class]):not(span):last-child
    margin-bottom 0 !important

  table
    margin 24px 0
  table th
    text-align left
    padding 19px 25px 14px
    color black
    fontify 15px 400 21px
    border-top 1px solid #e5e5e5
    @media $LandscapeMax
      fontify 14px 400 21px
  table td
    padding 19px 25px 14px
    color $dark
    fontify 15px 400 21px
    border-top 1px solid #e5e5e5
    @media $LandscapeMax
      fontify 14px 400 21px
  table tbody tr:last-child
    border-bottom 1px solid #e5e5e5

input::-webkit-input-placeholder
  color alpha(white, .5)
input::-moz-placeholder
  color alpha(white, .5)
input:-ms-input-placeholder
  color alpha(white, .5)

div[class\^="table-scroller"]
  overflow auto
  pos relative
  @media $desktopMax
    w 100%

.btn
  ib()
  bgc $orange
  border 0
  color white
  outline none
  cursor pointer
  size 116px
  pos relative
  fontify 15px 400 19px
  padding 25px 19px 56px
  text-align left
  background-image url('../i/btn-bg.png')
  background-blend-mode soft-light
  background-repeat no-repeat
  background-position top left
  transition .7s background
  @media $LandscapeMax
    size 90px
    padding 10px
    fontify 14px 400 18px
  &:after
    content ""
    pos absolute
    size 35px 17px
    l 20px
    b 30px
    background url("data:image/png;base64,") no-repeat center
    transition .7s opacity, .7s left
    @media $LandscapeMax
      dn()
  &:hover
    background-image url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAgwAAAEZAQMAAADBu+/5AAAABGdBTUEAALGPC/xhBQAAACBjSFJNAAB6JgAAgIQAAPoAAACA6AAAdTAAAOpgAAA6mAAAF3CculE8AAAABlBMVEX///////9VfPVsAAAAAXRSTlMD2e+J3AAAAAFiS0dEAIgFHUgAAAAJcEhZcwAACxIAAAsSAdLdfvwAAAAoSURBVHja7cEBAQAAAIIg/69uSEABAAAAAAAAAAAAAAAAAAAAAADcGkmLAAHdDymqAAAAAElFTkSuQmCC")
    &:after
      l 30px
      opacity 0
  &_dark
    bgc $dark
  &_small
    &:after
      b 41px
    
.common
  &__label
    opacity .25
    color white
    fontify 12px 600 18px
    db()
    mb 2px
  &__inp
    w 100%
    h 42px
    border 1px solid transparent
    bgc alpha(black, .13)
    color white
    padding 0 16px
    font-family $os
    fontify 15px 400 21px
    transition .5s border
    @media $LandscapeMax
      fontify 14px 400 21px
  &__textarea
    w 100%
    h 92px
    border 1px solid transparent
    overflow hidden
    resize none
    padding 11px 16px
    bgc alpha(black, .13)
    color white
    font-family $os
    fontify 15px 400 21px
    transition .7s border
    @media $LandscapeMax
      fontify 14px 400 21px
  &__sbm
    font-family $os
    mt 5px
  &__select
    h 36px
    bgc #fff
    border 2px solid #b6b6b6
    w auto
    cursor pointer
    transition .5s border
    &.warning
      border-color $orange
  &__placeholder
    pos absolute
    z-index -1
    l 0 
    t 0
    color white
    fontify 14px 700 50px
    letter-spacing .1em
    text-transform uppercase
    text-overflow ellipsis
    white-space nowrap
    overflow hidden
    max-width 100%
    span
      fontify 23px 400 50px
      pos relative
      t 0
  &__col
    w calc(50% - 16px)
    @media $LandscapeMax
      w 100%
      & + &
        mt 27px
  &__row
    &_flex
      display flex
      justify-content space-between
      pos relative
      w 100%
      @media $LandscapeMax
        flex-direction column
        justify-content flex-start
    & + &
      mt 27px
  &__fileover
    w 100%
    pos relative
    pt 25px
  &__file
    pos absolute
    w 0
    h 0
    l 0
    t 0
    opacity 0
    &name
      ib()
      pos relative
      l 48px
      cursor pointer
      color white
      fontify 15px 400 21px
      border-bottom 2px solid $orange
      transition .5s border
      @media $LandscapeMax
        fontify 14px 400 21px
        l 33px
      &.choosed,
      &:hover
        border-color transparent
      &:before
        content ""
        pos absolute
        l -38px
        t 0
        size 24px 23px
        background url("data:image/png;base64,") no-repeat center
        @media $LandscapeMax
          l -33px
      &.choosed + ^[-1]delete
        visibility visible
        cursor pointer
      &.choosed:after
        content ""
    &delete
      ib()
      visibility hidden
      mt 6px
      ml 58px
      size 10px
      background url("data:image/png;base64,") no-repeat center

.fakecheckbox
  ib()
  pos relative
  mt 7px
  &__label
    ib()
    color white
    fontify 15px 400 21px
    pos relative
    pl 33px
    pt 7px
    @media $LandscapeMax
      fontify 14px 400 21px
    &:before 
      content ""
      pos absolute
      l -1px
      t 50%
      mt -5px
      size 18px
      border 1px solid #ececec
      cursor pointer
      transition .5s border
    &:after
      content ""
      pos absolute
      size 14px 10px
      background url("data:image/png;base64,") no-repeat center
      l 2px
      t 50%
      mt 0px
      opacity 0
      cursor pointer
      transition .5s opacity
    a:not([class])
      color white
  &__truecheck
    pos absolute
    l 0
    t 0
    w 0
    h 0
    border 0
    opacity 0
    &:checked + ^[0]__label:after
      opacity 1
    &.warning + ^[0]__label:before 
      border-color $orange

input.warning,
textarea.warning
  border-color $orange

.hidden-block
  display none