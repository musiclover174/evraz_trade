html,
body
  font-family $os
  color black
body
  overflow-x hidden
    
.content
  padding 160px 65px 0
  h1:not([class])
    color white
    max-width 830px
    fontify 72px 400 76px
    margin 63px 0 35px
    @media $lDesktopMedium
      fontify 60px 400 64px
      max-width 720px
    @media $desktopMin
      fontify 48px 400 52px
    @media $LandscapeMedium
      fontify 36px 400 42px
  h2:not([class])
    color $dark
    fontify 42px 400 46px
    margin 63px 0 28px
    @media $desktopMax
      fontify 38px 400 42px
    @media $lDesktopSmall
      fontify 36px 400 40px
    @media $LandscapeMedium
      fontify 30px 400 34px
      margin 45px 0 28px
  h3:not([class])
    color $dark
    fontify 30px 400 36px
    margin 65px 0 38px
    @media $LandscapeMedium
      fontify 26px 400 34px
      margin 45px 0 28px
  h4:not([class])
    color $dark
    fontify 24px 400 34px
    margin 43px 0 16px
  img:not([class])
    margin 5px 0
    max-width 100%
  ul:not([class])
    list-style  none
    margin  23px 0
  ul:not([class]) li
    pl 30px
    pos relative
    fontify 20px 400 27px
    @media $lDesktopSmall
      fontify 17px 400 24px
  ul:not([class]) li + li
    mt 25px
  ul:not([class]) li:before
    content ""
    pos absolute
    l 0
    t 13px
    size 14px 1px
    bgc $dark
  ol:not([class])
    list-style none
    margin 23px 0
    counter-reset myCounter
  ol:not([class]) li
    pl 30px
    pos relative
    fontify 20px 400 27px
    @media $lDesktopSmall
      fontify 17px 400 24px
  ol:not([class]) li + li
    mt 24px
  ol:not([class]) li:before
    counter-increment myCounter
    content counter(myCounter)"."	
    pos absolute
    l 0
    t 0
    w 30px
    pr 9px
    color $orange
    fontify 20px 400 27px
    @media $lDesktopSmall
      fontify 17px 400 24px
  p:not([class])
    color $dark
    fontify 20px 400 27px
    margin 23px 0
    @media $lDesktopSmall
      fontify 17px 400 24px
  a:not([class])
    color $green
    border-bottom 1px solid
    transition .5s border
    &:hover
      border-color transparent
 
  *:not([class]):not(span):first-child
    mt 0 !important
  *:not([class]):not(span):last-child
    margin-bottom 0 !important

  table
    margin 30px 0
    border 2px solid #9db038
  table th
    text-align left
    padding 16px 24px
    color $dark
    bgc $orange
    fontify 15px 400 21px
    border 1px solid #f0f3e1
  table td
    padding 18px 24px
    color $dark
    fontify 15px 400 21px
    border 1px solid #f0f3e1

input::-webkit-input-placeholder
  color alpha(white, .5)
input::-moz-placeholder
  color alpha(white, .5)
input:-ms-input-placeholder
  color alpha(white, .5)

div[class\^="table-scroller"]
  overflow auto
  pos relative
  @media $desktopMax
    w 100%

.btn
  ib()
  bgc $orange
  border 0
  color white
  outline none
  cursor pointer
  size 116px
  pos relative
  fontify 15px 400 19px
  padding 25px 19px 56px
  text-align left
  background-image url('../i/btn-bg.png')
  background-blend-mode soft-light
  background-repeat no-repeat
  background-position top left
  transition .7s background
  &:after
    content ""
    pos absolute
    size 35px 17px
    l 20px
    b 30px
    background url("data:image/png;base64,") no-repeat center
    transition .7s opacity, .7s left
  &:hover
    background-image url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAgwAAAEZAQMAAADBu+/5AAAABGdBTUEAALGPC/xhBQAAACBjSFJNAAB6JgAAgIQAAPoAAACA6AAAdTAAAOpgAAA6mAAAF3CculE8AAAABlBMVEX///////9VfPVsAAAAAXRSTlMD2e+J3AAAAAFiS0dEAIgFHUgAAAAJcEhZcwAACxIAAAsSAdLdfvwAAAAoSURBVHja7cEBAQAAAIIg/69uSEABAAAAAAAAAAAAAAAAAAAAAADcGkmLAAHdDymqAAAAAElFTkSuQmCC")
    &:after
      l 30px
      opacity 0
  &_dark
    bgc $dark
    
.common
  &__title
    color $dark
    fontify 44px 400 54px
    mb 25px
  &__input
    w 100%
    h 48px
    border 0
    border-bottom 1px solid white
    bgc transparent
    color white
    font-family $os
    font-size 17px
    line-height 400
    transition .5s border
    @media $lDesktopSmall
      font-size 14px
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      dn()
  &__textarea
    w 100%
    h 116px
    overflow hidden
    resize none
    padding 11px 18px
    border 2px solid #ececec
    bgc transparent
    color $dark
    font-family $os
    fontify 14px 400 24px
    transition .5s border
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      dn()
  &__sbm
    font-family $os
    mt 5px
  &__select
    h 36px
    bgc #fff
    border 2px solid #b6b6b6
    w auto
    cursor pointer
    transition .5s border
    &.warning
      border-color #ed1c24
  &__placeholder
    pos absolute
    z-index -1
    l 0 
    t 0
    color white
    fontify 14px 700 50px
    letter-spacing .1em
    text-transform uppercase
    text-overflow ellipsis
    white-space nowrap
    overflow hidden
    max-width 100%
    @media $lDesktopSmall
      fontify 12px 700 50px
    span
      fontify 23px 400 50px
      pos relative
      t 0
  &__col
    w calc(50% - 25px)
    &_wide
      w calc(100% - 212px)
    &_short
      w 162px
    &_iroad
      @media $LandscapeMedium
        &:first-child
          dn()
    @media $LandscapeMedium
      w 100%
      & + &
        mt 22px
  &__row
    display flex
    justify-content space-between
    pos relative
    w 100%
    @media $LandscapeMedium
      flex-direction column
    & + &
      mt 32px
      @media $LandscapeMedium
        mt 22px
  &__fileover
    w 100%
    pos relative
    pt 14px
    pb 14px
    border-bottom 1px solid white
    transition .5s border
    &.warning
      border-color #ed1c24
    & + &
      mt 20px
  &__file
    pos absolute
    w 0
    h 0
    l 0
    t 0
    opacity 0
    &name
      ib()
      pos relative
      l 26px
      cursor pointer
      color white
      fontify 14px 700 14px
      letter-spacing .1em
      text-transform uppercase
      border-bottom 1px solid alpha(white, .25)
      transition .5s border
      @media $lDesktopSmall
        font-size 12px
      &.choosed,
      &:hover
        border-color transparent
      &:after
        content "*"
        pos absolute
        r -11px
        fontify 23px 400 14px
        t -3px
      &:before
        content ""
        pos absolute
        l -24px
        t 3px
        size 16px 8px
        background url("data:image/png;base64,") no-repeat center
      &.choosed + ^[-1]delete
        visibility visible
        cursor pointer
      &.choosed:after
        content ""
    &delete
      ib()
      visibility hidden
      mt 3px
      ml 32px
      size 9px
      background url("data:image/png;base64,") no-repeat center

.fakecheckbox
  ib()
  pos relative
  margin 10px 0
  &__label
    ib()
    color white
    fontify 14px 400 24px
    pos relative
    pl 37px
    pt 7px
    &:before 
      content ""
      pos absolute
      l 0px
      t 8px
      size 20px
      border 1px solid #ececec
      cursor pointer
      transition .5s border
    &:after
      content ""
      pos absolute
      size 16px 13px
      background url("data:image/png;base64,") no-repeat center
      l 4px
      t 13px
      opacity 0
      cursor pointer
      transition .5s opacity
  &__truecheck
    pos absolute
    l 0
    t 0
    w 0
    h 0
    border 0
    opacity 0
    &:checked + ^[0]__label:after
      opacity 1
    &.warning + ^[0]__label:before 
      border-color #ed1c24
    
.totop
  pos fixed
  z-index 5
  b 20px
  r 20px
  cursor pointer
  size 44px
  border-radius 50%
  box-shadow 0 6px 19px rgba(0, 0, 0, 0.12)
  visibility hidden
  opacity 0
  background url("data:image/png;base64,") no-repeat center #e76f17
  transition .5s visible, .5s box-shadow, .5s opacity
  &:hover
    box-shadow none
  &.visible
    visibility visible
    opacity 1

input.warning,
textarea.warning
  border-color #ff3a0f

.hidden-block
  display none